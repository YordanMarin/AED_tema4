package interfaz;

import javax.swing.*;

public class Formulario {

	public static void main(String[] args) {
		Formulario f = new Formulario();
		f.ventana();

	}
	
	public void ventana() {
		//creando ventana
		JFrame frame = new JFrame();
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.setVisible(true);
		
	}

}
